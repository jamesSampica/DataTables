<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <PackageId>Wetware.DataTables</PackageId>
    <Version>1.0.0-rc</Version>
    <Company>Wetware</Company>
    <Authors>James Sampica</Authors>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>Models for binding JQuery DataTables requests to an ASP.NET backend</Description>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/jamesSampica/DataTables</RepositoryUrl>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <IncludeSymbols>true</IncludeSymbols>
    <EmbedAllSources>true</EmbedAllSources>
    <DebugSymbols>true</DebugSymbols>
  </PropertyGroup>
</Project>
